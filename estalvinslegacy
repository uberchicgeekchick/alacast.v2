&lt;ul style="margin-top: 0px; margin-right: 0px; margin-bottom: 0px; margin-left: 0px; padding-left: 2em; list-style-type: circle; "&gt;
&lt;li style="margin-top: 0px; margin-right: 0px; margin-bottom: 0px; margin-left: 0px; padding-top: 0px; padding-right: 0px; padding-bottom: 0px; padding-left: 0px; "&gt;&lt;a style="text-decoration: none; color: rgb(39, 99, 165); " href="http://escapepod.org/"&gt;Escapepod&lt;/a&gt;&amp;nbsp;- Science Fiction&lt;/li&gt;
&lt;li style="margin-top: 0px; margin-right: 0px; margin-bottom: 0px; margin-left: 0px; padding-top: 0px; padding-right: 0px; padding-bottom: 0px; padding-left: 0px; "&gt;&lt;a style="text-decoration: none; color: rgb(39, 99, 165); " href="http://pseudopod.org/"&gt;Pseudopod&lt;/a&gt;&amp;nbsp;- Horror&lt;/li&gt;
&lt;li style="margin-top: 0px; margin-right: 0px; margin-bottom: 0px; margin-left: 0px; padding-top: 0px; padding-right: 0px; padding-bottom: 0px; padding-left: 0px; "&gt;&lt;a style="text-decoration: none; color: rgb(39, 99, 165); " href="http://podcastle.org/"&gt;Podcastle&lt;/a&gt;&amp;nbsp;- Fantasy&lt;/li&gt;
